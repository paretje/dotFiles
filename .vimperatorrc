set hintchars=hjklasdf
set hintmatching=firstletters
set editor=urxvt\ -e\ nvim
set complete=l
set defsearch=duckduckgo
set toolbars=addons,nobookmarks,nomenu,nonavigation,tabs
set focuscontent=true

noremap d <C-d>
noremap x :tabclose<Return>
noremap u <C-u>
noremap X :undo<Return>
noremap s :stop<Return>
noremap j 2j
noremap k 2k

cnoremap <C-p> <up>
cnoremap <C-n> <down>

command! q winclose

" vim: set ft=vimperator:
